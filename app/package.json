{
  "name": "another",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack-dev-server --config webpack.dev.js --open",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage:watch": "jest --watchAll --coverage --collectCoverage --color",
    "lint": "eslint ."
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/src/scripts/lib/fontAwesomeIcons.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/scripts/data/*.js",
      "**/src/scripts/*.js",
      "**/src/scripts/DOM/*.js",
      "!**/src/scripts/agentChart.js",
      "!**/src/scripts/DOM/*",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75
      }
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-jest": "^25.4.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.3",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.4.0",
    "mini-css-extract-plugin": "^0.9.0",
    "netlify-cli": "^2.50.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "style-loader": "^1.2.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "chart.js": "^2.9.3",
    "font-awesome": "^4.7.0",
    "gl-matrix": "^3.3.0",
    "jspdf": "^1.5.3",
    "nouislider": "^14.5.0",
    "seedrandom": "^3.0.5",
    "wnumb": "^1.2.0"
  }
}
